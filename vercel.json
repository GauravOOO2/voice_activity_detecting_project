{
    "version": 2,
    "functions": {
        "api/**/*.py": {
            "runtime": "@vercel/python",
            "maxDuration": 300
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/api/index.py"
        },
        {
            "src": "/transcript",
            "dest": "/api/transcript.py"
        },
        {
            "src": "/setup_db",
            "dest": "/api/setup_db.py"
        }
    ]
}